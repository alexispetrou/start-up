    "def map_levels(cell):\n",
    "    global level_mapping\n",
    "\n",
    "    if pd.isna(cell):\n",
    "        return None\n",
    "\n",
    "    parts = str(cell).split(',')\n",
    "    levels = []\n",
    "\n",
    "    for part in parts:\n",
    "        cleaned = part.strip()\n",
    "\n",
    "        mapped = level_mapping.get(cleaned)\n",
    "        if mapped is not None:\n",
    "            levels.append(mapped)\n",
    "\n",
    "    return levels if levels else None"
   ]
